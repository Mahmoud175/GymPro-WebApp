    <section class="section-2">
        <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b8152-b3e9f4f0" class="w-layout-vflex flex-block">
            <a href="#" id="pi" class="account-button clicked w-button" onclick="piContainer('PI')">Personal Info</a>
            <a href="#" id="se" class="account-button w-button" onclick="seContainer('SE')">Sessions</a>
            <a href="#" id="cu" class="account-button w-button"onclick="cuContainer('CU')">Customers</a>
            <a href="#" id="cp" class="account-button w-button" onclick="cpContainer('CP')">Change Password</a>
            <a href="/logout" id="LO" onclick="return logOut(this)" class="account-button w-button">Log Out</a>
        </div>
        <div id = "PI" class="w-layout-blockcontainer info-container w-container">
            <h1>Personal Information</h1>
            <div class="form-block-2 w-form">
                <form id="email-form" name="email-form" data-name="Email Form" method="get" class="form-2"
                    data-wf-page-id="657f1f46c789b69eb3e9f4f0"
                    data-wf-element-id="bb92477b-2511-bc47-f994-0cc92d0b815f">
                    <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b8160-b3e9f4f0" class="info-div"><label for="name-3"
                            class="info-label">First Name:</label>
                        <div class="text-block-2"><%= currentUser.name.firstName %></div>
                    </div>
                    <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b8164-b3e9f4f0" class="info-div"><label for="name"
                            class="info-label">Last Name:</label>
                        <div class="text-block-2"><%= currentUser.name.lastName %></div>
                    </div>
                    <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b8168-b3e9f4f0" class="info-div"><label for="name"
                            class="info-label">Email Address:</label>
                        <div class="text-block-2"><%= currentUser.email %></div>
                    </div>
                    <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b816d-b3e9f4f0" class="info-div"><label for="name"
                            class="info-label">Phone:</label>
                        <div class="text-block-2"><%= currentUser.phone %></div>
                    </div>
                    <div id="w-node-bb92477b-2511-bc47-f994-0cc92d0b8176-b3e9f4f0" class="info-div"><label for="Goal"
                            class="info-label">Assigned Customers:</label>
                        <div class="text-block-2"><%= assignedCustomers.length  %></div>
                    </div>
                </form>
            </div>
        </div>


        <div id="NSE"
            class="w-layout-blockcontainer no-customers-container w-container" style="display: none;">
            <h1 class="heading-2 no-program">No Sessions Scheduled.</h1>
        </div>

        <div id="SE"
            class="w-layout-blockcontainer sessions-container w-container" style="display: none;">
            <h1 class="heading-2">Sessions</h1>
            <div class="sessions-div">
                <div class="sessions">
                    <% assignedCustomers.forEach((customer, index) => { %>
                        <% if(JSON.parse(customer.membershipData).nextMeeting.meetingDateTime.meetingDate != null){ %>
                        <div class="session">
                            <h4>Session <%= index + 1 %>: <%= customer.name.firstName %> <%= customer.name.lastName %></h4>
                            <div>Meeting Date:<%= JSON.parse(customer.membershipData).nextMeeting.meetingDateTime.meetingDate%> <%= JSON.parse(customer.membershipData).nextMeeting.meetingDateTime.meetingtime%></div>
                            
                            <% if(JSON.parse(customer.membershipData).nextMeeting.meetingLink != null) {%>
                            <div>Meeting Link: <%= JSON.parse(customer.membershipData).nextMeeting.meetingLink  %></div>
                            <% }else{ %>
                                <div>Meeting Link: No Link Yet</div>

                                <% } %>
                        </div>
                        <% } %>
                            

                    <% }); %>
                </div>
            </div><a href="/schedule-session-dsadsad-CexMk0xDMJR2n1Jfp3zVqLasdsadsaTz-dsadsadsacoach" class="add-session w-button">Add Session</a>
            
        </div>
        
        <% if(assignedCustomers.length != 0) { %>
            <div id="CU" class="w-layout-blockcontainer customers-container w-container" style="display: none;">
            <h1 class="heading-2">Assigned Customers</h1>
            <div class="workout-div">
                <div class="workouts">
                    <% assignedCustomers.forEach((customer, index) => { %>
                        <div class="customer-div" key="<%= customer.userId %>" onclick="selectCustomer(this)" style="cursor: pointer; border: 2px solid transparent; width: auto;">
                          <h4>Customer <%= index + 1 %>: <%= customer.name.firstName %> <%= customer.name.lastName %></h4>
                          <ul role="list">
                            
                            <li>Weight: <%= customer.weight%></li>
                            <li>Height: <%= customer.height%></li>
                            <li>Gender: <%= customer.gender%></li>
                            <li>Goal: <%= customer.goal%></li>
                            <li>Membership: <%= JSON.parse(customer.membershipData).type%></li>
                            <li>SessionsLeft: <%= JSON.parse(customer.membershipData).sessionsLeft%></li>
                            <li>Phone: <%= customer.phone%></li>

                            <% if (JSON.parse(customer.membershipData) && JSON.parse(customer.membershipData).workoutPlan && JSON.parse(customer.membershipData).workoutPlan.length > 0) {%>
                                <li>Workout Plan: Created</li>
                                <% }else{ %>
                                <li>Workout Plan: Not Created</li>
                                    <% } %>

                            <% if (JSON.parse(customer.membershipData) && JSON.parse(customer.membershipData).dietPlan && JSON.parse(customer.membershipData).dietPlan.length > 0) {%>
                                <li>Diet Plan: Created</li>
                                <% }else{ %>
                                <li>Diet Plan: Not Created</li>
                                <% } %>
                            
                          </ul>
                        </div>
                      <% }); %>
                </div>
                <a href="#" id="editWorkoutPlan" class="add-session w-button" style="display: none;" onclick="editWorkoutPlan()">Edit Workout plan</a>
                <a href="#" id="editDietPlan" class="remove-session w-button" style="display: none;" onclick="editDietPlan()">Edit Diet plan</a>
            </div>
        </div>
        <% }else{ %>
            <div id="CU"
                class="w-layout-blockcontainer no-customers-container w-container" style="display: none;">
            <h1 class="heading-2 no-program">No Customers Yet.</h1>
            </div>
            <% } %>
        <div id="CP"
            class="w-layout-blockcontainer change-pass-container w-container" style="display: none;">
            <h1 class="heading-2 no-program">Change Password</h1>
            <div class="form-block-4 w-form">
                <form id="email-form-2" name="email-form-2" data-name="Email Form 2" method="post" class="form-3"
                    data-wf-page-id="657dd8978d8a314907dc101f"
                    data-wf-element-id="9deef4a1-11bf-ad48-e839-ef1b1c5a38a6">
                    <label for="name">New Password</label>
                        
                        <input type="password" class="change-field w-input" maxlength="256" name="name"
                        data-name="Name" placeholder="" id="password" />
                        
                        <label for="email">Confirm Password</label>
                        
                        <input type="password" class="change-field w-input" maxlength="256" name="email" data-name="Email"
                        placeholder="" id="confirmPassword" required="" />
                        
                        <input type="submit" value="Confirm"
                        data-wait="Please wait..." class="submit-button-4 w-button" />
                </form>
            </div>
        </div>
    </section>
