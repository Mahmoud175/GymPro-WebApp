            <h1 class="heading-2">Customers</h1>
            <div class="workout-div">
                <div class="workouts">  
                    <% gymCustomers.forEach((customer, index) => { %>
                        <div class="customer-div" key="<%= customer.userId %>" onclick="selectCustomer(this)" style="cursor: pointer; border: 2px solid transparent; width: auto;">
                          <h4>Customer <%= index + 1 %>: <%= customer.name.firstName %> <%= customer.name.lastName %></h4>
                          <ul role="list">
                            
                            <% if(customer.membershipData != null) {%>
                            <li>Email: <%= customer.email%></li>
                            <li class="membership <%= customer.userId %>">Membership: <%= JSON.parse(customer.membershipData).type%></li>
                            <li>Goal: <%= customer.goal%></li>
                            <% if(customer.personalCoach != null) {%>
                            <li class="personalCoach <%= customer.userId %>">personalCoach: <%= personalCoaches[index].name.firstName%>  <%= personalCoaches[index].name.lastName%></li>
                            <% }else{ %>
                            <li class="personalCoach <%= customer.userId %>">personalCoach: None</li>
                            <% } %>
                            <li>sessionsLeft: <%= JSON.parse(customer.membershipData).sessionsLeft %></li>
                            <% }else{ %>
                            <li>Email: <%= customer.email%></li>
                            <li class="membership <%= customer.userId %>">Membership: None</li>
                            <li>Goal: <%= customer.goal%></li>
                            <li class="personalCoach <%= customer.userId %>">personalCoach: None</li>
                            <li>sessionsLeft: 0</li>

                            <% } %>
                            
                          </ul>
                        </div>
                      <% }); %>
                </div>
